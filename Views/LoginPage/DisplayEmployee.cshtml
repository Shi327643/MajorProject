@model TravelBookingProject.ViewModel.TravelViewModel
@{
    ViewBag.Title = "DisplayEmployee";
}

<h2>DisplayEmployee</h2>
<table class="table table-striped">
    <tr>
        <th>Emp Id</th>
        <th>Emp Name</th>
        <th>Country</th>
        <th>City</th>
        <th>GCM Id</th>
        <th></th>
    </tr>
    @foreach (var item in Model.EmpList)
    {
        <tr>
            <td>@item.EmpId</td>
            <td>@item.EmpName</td>
            <td>@item.Country</td>
            <td>@item.City</td>
            <td>@item.GCMId</td>


        </tr>

    }
</table>


