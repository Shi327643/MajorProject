@model TravelBookingProject.ViewModel.TravelViewModel
@{
    ViewBag.Title = "DisplayCity";
}

<h2>DisplayCity</h2>
<table class="table table-striped">
    <tr>
        <th>City Id</th>
        <th>City Name</th>
        <th>CountryID</th>
        <th></th>
    </tr>
    @foreach (var item in Model.cityList)
    {
        <tr>
            <td>@item.CityId</td>
            <td>@item.CityName</td>
            <td>@item.CountryId</td>
           


        </tr>

    }
</table>
